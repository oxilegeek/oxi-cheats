		local dummy = game.Workspace.Map.snow:FindFirstChild("training area").dummy:GetChildren()
		for i = 1, #dummy do
			if i == 3 then
				dummy[i].Name = "Dummy3"
			end
		end

		local training = game.Workspace.pleaseno:GetChildren()
		for i = 1, #training do
			if i == 3 then
				training[i].Name = "cheatSpot"
			end
		end
		if value then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0
			local terrain = game.Workspace.Map.forest.terrain.forest_terrain3:GetChildren()

			for i = 1, #terrain do
				terrain[i].CanCollide = false
			end
			if game.Players.LocalPlayer.leaderstats.Level.Value >= 5000 then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace.Map.snow:FindFirstChild("training area").dummy.Dummy3.HumanoidRootPart.Position.X, game.Workspace.Map.snow:FindFirstChild("training area").dummy.Dummy3.HumanoidRootPart.Position.Y - 5, game.Workspace.Map.snow:FindFirstChild("training area").dummy.Dummy3.HumanoidRootPart.Position.Z)
			else
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace.pleaseno.cheatSpot.HumanoidRootPart.Position)
			end
		else
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = 60
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(272.455, -12.6042, 3052.27)	
			local terrain = game.Workspace.Map.forest.terrain.forest_terrain3:GetChildren()

			for i = 1, #terrain do
				terrain[i].CanCollide = true
			end
		end
		autofarm = value
	end)


